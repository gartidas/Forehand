{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "ConnectionStrings:ForehandContext": "Server=.;Database=ForehandDb;Trusted_Connection=True;MultipleActiveResultSets=true",

  "JwtOptions:Issuer": "Forehand",
  "JwtOptions:RefreshTokenLifeTime": "30:00:00:00",
  "JwtOptions:TokenLifetime": "00:30:00",
  "JwtOptions:Secret": "Secret_Is_In_User_Secrets_File"
}
